      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestCloudv2.Documents.DCM_Items.DCM_Item_New.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="NV_DCM_Items_New_Main">

    <Grid Name="GR_Navigation">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Margin="20" Click="EV_MD_Headboard">Cabecera</Button>
        <Button Grid.Column="1" Margin="20" Click="EV_MD_Movements">Lineas</Button>
        <Button Grid.Column="2" Margin="20" Click="EV_MD_Summary">Resumen</Button>
        <Button Grid.Column="7" Margin="20" Click="EV_CT_Menu">Volver</Button>
    </Grid>
</Page>
