<Page x:Class="GestCloudv2.Main.Controller.CT_Common"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestCloudv2.Main.Controller"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="CT_Common">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="0.25*"/>
        </Grid.RowDefinitions>
        <Grid Name="GR_Content">
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="4*"/>
            </Grid.RowDefinitions>
            <Grid>
                <Frame Name="TopSide"/>
            </Grid>
            <Frame Grid.Row="1" Name="FR_Subcontent" NavigationUIVisibility="Hidden"/>
        </Grid>
        <Frame Grid.Row="2" Name="BotSide" NavigationUIVisibility="Hidden"/>
    </Grid>
</Page>
