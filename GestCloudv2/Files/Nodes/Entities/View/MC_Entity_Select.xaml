<Page x:Class="GestCloudv2.Files.Nodes.Entities.View.MC_Entity_Select"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestCloudv2.Files.Nodes.Entities.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="MC_Entity_Select">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.5*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="0.5*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="0.5*"/>
        </Grid.ColumnDefinitions>
        <Button Name="BT_EntityNew" Grid.Column="1" VerticalAlignment="Center">
            <TextBlock TextAlignment="Center">Nueva<LineBreak/>Persona</TextBlock>
        </Button>
        <Button Name="BT_EntityExamine" Grid.Column="3" VerticalAlignment="Center">
            <TextBlock TextAlignment="Center">Seleccionar<LineBreak/>Persona</TextBlock>
        </Button>
    </Grid>
</Page>
