<Page x:Class="GestCloudv2.Files.Nodes.Stores.StoreMenu.View.TS_STR_Menu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestCloudv2.Files.Nodes.Stores.StoreMenu.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="TS_STR_Menu">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Margin="10" Click="EV_StoreNew">
            <TextBlock TextAlignment="Center">Nuevo<LineBreak/>Almacén</TextBlock>
        </Button>
        <Button Name="BT_StoreLoad" IsEnabled="false" Margin="10" Grid.Column="1" Click="EV_CT_StoreLoad">
            <TextBlock TextAlignment="Center">Examinar<LineBreak/>Almacén</TextBlock>
        </Button>
        <Button Name="BT_StoreLoadEdit" IsEnabled="False" Margin="10" Grid.Row="1" Click="EV_CT_StoreLoadEditable">
            <TextBlock TextAlignment="Center">Editar<LineBreak/>Almacén</TextBlock>
        </Button>
    </Grid>
</Page>
