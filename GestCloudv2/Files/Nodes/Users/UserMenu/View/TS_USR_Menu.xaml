<Page x:Class="GestCloudv2.Files.Nodes.Users.UserMenu.View.TS_USR_Menu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestCloudv2.Files.Nodes.Users.UserMenu.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="TS_USR_Menu">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Grid.Column="1" Grid.Row="0" Margin="20" Click="EV_CT_UserNew">Nuevo Usuario</Button>
        <Button Name="ViewUserButton" IsEnabled="False" Grid.Column="1" Grid.Row="1" Margin="20" Click="ViewUserEvent">Ver Usuario</Button>
        <Button Name ="EditUserButton" IsEnabled="False" Grid.Column="1" Grid.Row="2" Margin="20" Click="EditUserEvent">Editar Usuario</Button>
    </Grid>
</Page>
